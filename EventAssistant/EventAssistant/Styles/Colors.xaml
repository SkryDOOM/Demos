<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:EventAssistant.Styles">

    <Color x:Key="PrimaryColor">#40514e</Color>
    <SolidColorBrush x:Key="PrimaryColorBrush" Color="{StaticResource PrimaryColor}"/>

    <Color x:Key="SecondaryColor">#11999e</Color>
    <SolidColorBrush x:Key="SecondaryColorBrush" Color="{StaticResource SecondaryColor}"/>

    <Color x:Key="TertiaryColor">#30e3ca</Color>
    <SolidColorBrush x:Key="TertiaryColorBrush" Color="{StaticResource TertiaryColor}"/>

    <Color x:Key="BackgroundColor">#ffffff</Color>
    <SolidColorBrush x:Key="BackgroundColorBrush" Color="{StaticResource BackgroundColor}"/>

    <Color x:Key="LightColor">#e4f9f5</Color>
    <SolidColorBrush x:Key="LightColorBrush" Color="{StaticResource LightColor}"/>

    <Color x:Key="IconsColor">#ff3d00</Color>
    <SolidColorBrush x:Key="IconsColorBrush" Color="{StaticResource IconsColor}"/>

</ResourceDictionary>