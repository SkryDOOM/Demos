<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="BackgroundColor">#eeeeee</Color>
    <SolidColorBrush Color="{StaticResource BackgroundColor}" x:Key="BackgroundColorBrush"/>

    <Color x:Key="PrimaryColor">#448aff</Color>
    <SolidColorBrush Color="{StaticResource PrimaryColor}" x:Key="PrimaryColorBrush"/>

    <Color x:Key="SecondaryColor">#f57c00</Color>
    <SolidColorBrush Color="{StaticResource SecondaryColor}" x:Key="SecondaryColorBrush"/>

    <Color x:Key="SideMenuColor">#d8d8d8</Color>
    <SolidColorBrush Color="{StaticResource SideMenuColor}" x:Key="SideMenuColorBrush"/>

    <Color x:Key="DarkBoxColor">#bdbdbd</Color>
    <SolidColorBrush Color="{StaticResource DarkBoxColor}" x:Key="DarkBoxColorBrush"/>

    <Color x:Key="LightFontColor">#f5f5f5</Color>
    <SolidColorBrush Color="{StaticResource LightFontColor}" x:Key="LightFontColorBrush"/>

</ResourceDictionary>