<Page x:Class="MaterialDesign.Icons"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MaterialDesign"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Icons" Background="{StaticResource BackgroundColorBrush}">

    <Grid Margin="100 0 0 0">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Label Content="UNDER CONSTRUCTION" FontSize="25" HorizontalAlignment="Center"/>

        <MediaElement Grid.Row="1" x:Name="gif" MediaEnded="gif_MediaEnded" HorizontalAlignment="Center" VerticalAlignment="Top" Source="Style\Potato.gif" UnloadedBehavior="Manual" LoadedBehavior="Play" SpeedRatio="1" IsMuted="False" Width="600" Height="335"/>
    </Grid>
</Page>
